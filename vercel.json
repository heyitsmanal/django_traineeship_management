{
    "version": 2,
    "builds": [
      {
        "src": "student_management_system/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "ignorePipVersion": true,
          "maxLambdaSize": "15mb",
          "runtime": "python3.12"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "student_management_system/wsgi.py"
      }
    ]
  }
